@Generated(
    value = "org.mapstruct.ap.MappingProcessor",
    date = "2023-02-05T13:05:09+0530",
    comments = "version: 1.5.3.Final, compiler: Eclipse JDT (IDE) 1.3.1100.v20200828-0941, environment: Java 14.0.2 (Oracle Corporation)"
